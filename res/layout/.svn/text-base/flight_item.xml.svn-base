<?xml version="1.0" encoding="utf-8"?>
<LinearLayout
  xmlns:android="http://schemas.android.com/apk/res/android"
  android:orientation="vertical"
  android:layout_width="fill_parent"
  android:layout_height="wrap_content">
     <LinearLayout android:layout_width="fill_parent" android:id="@+id/linearLayout1" android:layout_height="wrap_content" android:layout_weight="1" android:paddingRight="10dp">
         <ImageView android:id="@+id/imageView1" android:src="@drawable/flight" android:layout_height="wrap_content" android:layout_gravity="center_vertical" android:layout_width="wrap_content" android:padding="3dp"></ImageView>
        <TextView android:textAppearance="?android:attr/textAppearanceMedium"  android:text="0000-00-00 00:00" android:layout_height="wrap_content" android:id="@+id/itemTime" android:layout_width="fill_parent" android:layout_weight="1"></TextView>
        <TextView android:textAppearance="?android:attr/textAppearanceMedium"  android:text="0" android:layout_height="wrap_content" android:id="@+id/itemInfo" android:gravity="right" android:layout_width="wrap_content"></TextView>
    </LinearLayout>
</LinearLayout>
